{
  "name": "eggtest",
  "version": "1.0.0",
  "description": "汇付宝收银台、SDK服务",
  "dependencies": {
    "egg": "^2.23.0",
    "egg-alinode": "^2.0.1",
    "egg-extend-ua": "^1.0.2",
    "egg-renderx": "^1.0.1",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.2.0",
    "format": "^0.2.2",
    "hy-message": "^1.4.0",
    "moment": "^2.24.0",
    "nacos": "^2.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "egg-bin": "^4.13.2",
    "egg-ci": "^1.13.0",
    "eslint": "^6.5.0",
    "eslint-config-egg": "^7.5.1",
    "husky": "^3.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git branch && npm run lint"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "npm run stop && egg-scripts start --title=eggtest",
    "dev": "npm run stop && egg-bin dev",
    "debug": "npm run stop && egg-bin debug",
    "stop": "egg-scripts stop --title=eggtest",
    "autod": "autod",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "ci": {
    "version": "10"
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "index.js",
    "app",
    "config",
    "app.js"
  ],
  "author": "zyy",
  "license": "MIT"
}
